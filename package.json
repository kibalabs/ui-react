{
  "name": "@kibalabs/ui-react",
  "version": "0.8.0",
  "description": "React UI components for building frontend projects at Kiba Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kibalabs/ui-react.git"
  },
  "homepage": "https://ui-react-docs.kibalabs.com",
  "bugs": "https://github.com/kibalabs/ui-react/issues",
  "scripts": {
    "build": "build-react-component",
    "start": "build-react-component --start",
    "start-dev": "build-react-component --start --dev",
    "clean": "rm -rf ./node_modules ./package-lock.json ./dist",
    "type-check": "type-check",
    "publish-next": "kiba-publish --next",
    "storybook": "start-storybook --docs --port 6006",
    "build-storybook": "build-storybook --docs --output-dir dist"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "@kibalabs/core": "^0.5.1",
    "@kibalabs/core-react": "^0.5.3",
    "lazysizes": "^5.2.1",
    "polished": "^4.0.5",
    "react-is": "^17.0.1",
    "react-markdown": "^5.0.3"
  },
  "devDependencies": {
    "@kibalabs/build": "0.6.3-next.9",
    "@storybook/addon-actions": "6.1.11",
    "@storybook/addon-docs": "6.1.11",
    "@storybook/addon-essentials": "6.1.11",
    "@storybook/addon-links": "6.1.11",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "6.1.11",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-is": "^17.0.0",
    "@types/styled-components": "5.1.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "5.2.1"
  },
  "peerDependencies": {
    "react": ">16.9.0",
    "react-dom": ">16.9.0",
    "styled-components": "^5.2.0"
  }
}
