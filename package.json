{
  "name": "@kibalabs/ui-react",
  "version": "0.9.8",
  "description": "React UI component library for building projects at Kiba Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kibalabs/ui-react.git"
  },
  "homepage": "https://ui-react-docs.kibalabs.com",
  "bugs": "https://github.com/kibalabs/ui-react/issues",
  "scripts": {
    "build": "build-react-component",
    "start": "build-react-component --start",
    "start-dev": "build-react-component --start --dev",
    "clean": "rm -rf ./node_modules ./package-lock.json ./dist",
    "lint": "lint",
    "lint-fix": "lint --fix",
    "type-check": "type-check",
    "publish-next": "kiba-publish --next",
    "docs": "start-storybook --static-dir ./.storybook/public --docs --port 6006",
    "build-docs": "build-storybook --static-dir ./.storybook/public --docs --output-dir dist"
  },
  "files": [
    "dist/*"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "@kibalabs/core": "^0.5.9",
    "@kibalabs/core-react": "^0.9.2",
    "lazysizes": "^5.3.2",
    "polished": "^4.2.2",
    "react-markdown": "^6.0.3",
    "remark-breaks": "^2.0.2"
  },
  "devDependencies": {
    "@kibalabs/build": "^0.8.3",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/react": "6.5.9",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "5.1.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.5"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 ",
    "react-dom": "^17.0.0 || ^18.0.0 ",
    "styled-components": "^5.3.1"
  }
}
